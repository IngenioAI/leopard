        "default": "model01"
      },
      {
        "id": "dataset",
        "title": "데이터셋",
        "type": "text",
        "default": "dataset01"
      },
      {
        "id": "unlearn_model_name",
        "title": "언러닝 모델 이름",
        "type": "text",
        "default": "unlearn01"
      },
      {
        "id": "forget_class_index",
        "title": "잊어버릴 클래스 인덱스",
        "type": "number",
        "default": 1
      }
    ]
  },
  {
    "id": "vggface2_dataset",
    "name": "VGGFace2 데이터 생성",
